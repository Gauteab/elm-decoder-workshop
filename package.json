{
  "name": "elm-decoder-workshop",
  "version": "1.0.0",
  "description": "A bunch of decoders for you to implement",
  "main": "index.js",
  "scripts": {
    "person": "elm-verify-examples && elm-test tests/VerifyExamples/PersonDecoder/PersonDecoder*.elm --watch",
    "person-with-phone": "elm-verify-examples && elm-test tests/VerifyExamples/PersonDecoder/PersonWithPhoneDecoder*.elm --watch",
    "person-with-maybe-phone": "elm-verify-examples && elm-test tests/VerifyExamples/PersonDecoder/PersonWithMaybePhoneDecoder*.elm --watch",
    "person-with-address": "elm-verify-examples && elm-test tests/VerifyExamples/PersonDecoder/PersonWithAddressDecoder*.elm --watch",
    "person-employee": "elm-verify-examples && elm-test tests/VerifyExamples/PersonDecoder/EmployeeDecoder*.elm --watch",
    "sum": "elm-verify-examples && elm-test tests/VerifyExamples/SumDecoder/*.elm --watch",
    "start": "elm-verify-examples && elm-test --watch",
    "solution": "cd solutions && elm-verify-examples && elm-test"
  },
  "author": "Gauteab",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5",
    "elm-test": "^0.19.1-revision7",
    "elm-verify-examples": "^5.2.0"
  }
}
